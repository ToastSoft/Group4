<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE configuration PUBLIC "-//ibatis.apache.org//DTD Config 3.0//EN" 
	"http://ibatis.apache.org/dtd/ibatis-3-config.dtd">
<configuration>

	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true" />
	</settings>
	<typeAliases>
		<typeAlias type="com.psas.entity.LogInfo" alias="logInfo" />
		<typeAlias type="com.psas.entity.UserInfo" alias="userInfo" />
		<typeAlias type="com.psas.entity.FineInfo" alias="fineInfo"/>
		<typeAlias type="com.psas.entity.CriminalReport" alias="criminal_Report"/>
	</typeAliases>

</configuration> 





